        {
          "version": 2,
          "builds": [
            {
              "src": "index.js",
              "use": "@vercel/node"
            }
          ],
          "routes": [
            {
              "src": "/(.*)",
              "dest": "/",
              "methods":["GET","POST","OPTIONS","PUT","DELETE","PATCH"],
              "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE, PATCH",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
              }
            }
          ]
        }